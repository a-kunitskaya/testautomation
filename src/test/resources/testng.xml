<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="suite" verbose="1" thread-count="2" parallel="methods">
    <listeners>
        <listener class-name="com.epam.cdp.base.listeners.ListenerOnTestStart"/>
    </listeners>


    <test name="Sum">
        <groups>
            <run>
                <exclude name="broken"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.cdp.tests.SumTest"/>
        </classes>
    </test>

    <test name="Sub">
        <classes>
            <class name="com.epam.cdp.tests.SubTest"/>
        </classes>
    </test>

    <test name="Mult">
        <parameter name="a" value="5"/>
        <parameter name="b" value="4"/>
        <parameter name="expectedResult" value="20"/>
        <classes>
            <class name="com.epam.cdp.tests.MultTest"/>
        </classes>
    </test>

    <test name="Div">
        <classes>
            <class name="com.epam.cdp.tests.factories.DivTestFactory"/>
        </classes>
    </test>
    <test name="Pow">
        <classes>
            <class name="com.epam.cdp.tests.PowTest"/>
        </classes>
    </test>

    <test name="IsPosivive">
        <classes>
            <class name="com.epam.cdp.tests.IsPositiveTest"/>
        </classes>
    </test>

    <test name="Tg">
        <classes>
            <class name="com.epam.cdp.tests.TgTest"/>
        </classes>
    </test>
</suite>

