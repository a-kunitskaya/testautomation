<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="FirstSuite" verbose="1" thread-count="2" parallel="methods" >
    <test name="Sum" >
        <classes>
            <class name="FirstSuite.SumTest" />
        </classes>
    </test>

    <test name="Sub" >
        <groups>
            <run>
                <include name="notBroken"/>
                <exclude name="broken"/>
            </run>
        </groups>
        <classes>
            <class name="FirstSuite.SubTest" />
        </classes>
    </test>

    <test name="Mult" >
        <parameter name="a" value="5";
        <parameter name="b" value="4";
        <parameter name="expectedResult" value="20";
        <classes>
            <class name="FirstSuite.multTest" />
        </classes>
    </test>

    <test name="Div" >
        <classes>
            <class name="Factory.divTestFactory" />
        </classes>
    </test>

    <test name="Pow" >
        <classes>
            <class name="FirstSuite.powTest" />
        </classes>
    </test>
</suite>

